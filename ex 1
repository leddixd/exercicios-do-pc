#include <stdio.h>

int main()
{
    int A, B, C, D;
    A=20;
    B=0;
    C=1;
    D=10;
    
    printf("A:");
    if ((A-B+D) >= (C)) {
        printf("True\n");
    }
    else{
        printf("False\n");
    }
    
    printf("B:");
    if ((A > (A + D)) || (C > B)) {
        printf("True\n");
    }
    else{
        printf("False\n");
    }
    
    printf("C:");
    if ((A * D) > (C + A) && (A > B)) {
        printf("True\n");
    }
    else{
        printf("False\n");
    }
    
    printf("D:");
    if ((((A+ B)/ C) >= 0.9) && ((B*D) < 0)) {
        printf("True\n");
    }
    else{
        printf("False\n");
    }
    printf("E:");
    if ((((A + C) / D) > 0.3 && ((B - C) < 0)) || ((C + A) > D)) {
        printf("True\n");
    } else {
        printf("False\n");
    }
    printf("D:");
    if ((((A + C) / D) > 0.3 && ((B - C) < 0)) || ((C + A) > D)) {
        printf("True\n");
    } else {
        printf("False\n");
    }
    return 0;
}
